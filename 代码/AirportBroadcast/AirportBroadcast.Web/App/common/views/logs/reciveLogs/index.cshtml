<div ng-controller="common.views.logs.reciveLogs.index as vm">
    <div class="row margin-bottom-5">
        <div class="col-xs-6">
            <div class="page-head">
                <div class="page-title">
                    <h1>
                        <span>数据接收日志</span>
                    </h1>
                </div>
            </div>
        </div>
    </div>

    <div class="portlet light margin-bottom-0">
        <div class="portlet-body">
            <div class="form-body" enter-key="vm.getUsers()">
                <div class="row margin-bottom-10">
                    
                    <div class="col-md-3">
                        <div class="form-group">
                            <label class="control-label">操作</label>
                            <button class="btn btn-default btn-block" ng-click="vm.getAll()"><i class="fa fa-refresh"></i> @L("Refresh")</button>
                        </div>
                    </div>
                </div>
            </div>
            <div busy-if="vm.loading">
                <div ui-grid="vm.options" ui-grid-resize-columns></div>
            </div>
        </div>
    </div>
</div>  
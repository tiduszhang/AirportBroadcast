<div id="addModal"> 
    <div class="modal-header">
        <h4 class="modal-title">
            <span>播放列表</span>
            <button type="button" class="pull-right btn btn-danger" button-busy="vm.loading" ng-click="vm.cancel()">清空播放列表</button>
        </h4>
    </div>
    <div class="modal-body">
        @*<div busy-if="vm.loading">
            <div ui-grid="vm.options" ui-grid-resize-columns></div>
        </div>*@
        <form class="form-inline">
            <div class="form-group">
                <label class="control-label">延误时间</label>
                <input type="text" id="startTime12" name="startTime12"
                       class="form-control timepicker" ng-model="dateOne">
            </div>
            <div class="form-group">
                <label class="control-label">延误时间</label>
                <input type="time" class="form-control">
            </div>
        </form>
        

    </div>
    <div class="modal-footer">
        <button type="button" button-busy="vm.loading" class=" btn btn-default" ng-click="vm.cancel()">刷新</button>
        <button type="button" button-busy="vm.loading" class=" btn btn-default" ng-click="vm.cancel()">关闭</button>
    </div>
</div>
